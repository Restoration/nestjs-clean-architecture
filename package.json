{
  "name": "nestjs-clean-architecture",
  "version": "0.0.1",
  "description": "",
  "author": "devark.inc",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=\"development\" nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:stg": "cross-env NODE_ENV=\"staging\" node dist/main",
    "start:prod": "cross-env NODE_ENV=\"production\" node dist/main",
    "lint:fix": "eslint \"**/*.ts\" --fix --ignore-pattern node_modules/",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./jest-e2e.json",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "seed:generate": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:generate -c seed -n",
    "seed:run": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:run -c seed",
    "seed:revert": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:revert -c seed",
    "migration:generate": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:run": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:revert"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/typeorm": "^8.0.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "tslib": "^2.3.1",
    "typeorm": "^0.2.37"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.9",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
